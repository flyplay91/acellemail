{
  "name": "rdok/elasticemail-php",
  "description": "Integration to the Elastic Email API http://api.elasticemail.com/public/help",
  "license": "MIT",
  "authors": [
    {
      "name": "Rizart Dokollari",
      "email": "r.dokollari@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "whatthejeff/nyancat-phpunit-resultprinter": "^1.2",
    "vlucas/phpdotenv": "^2.2",
    "fzaninotto/Faker": "^1.6",
    "php-vcr/php-vcr": "^1.2",
    "php-vcr/phpunit-testlistener-vcr": "^1.1",
    "fzaninotto/faker": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "ElasticEmail\\": "src/ElasticEmail/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ElasticEmail\\": "src/ElasticEmail/",
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
